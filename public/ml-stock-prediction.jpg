<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <rect width="400" height="300" fill="#f3f4f6"/>
  <rect x="50" y="50" width="300" height="200" fill="#dc2626" rx="8"/>
  
  <!-- Stock price chart -->
  <polyline points="70,200 90,180 110,190 130,160 150,170 170,140 190,150 210,120 230,130 250,100 270,110 290,90 310,100 330,80" 
            fill="none" stroke="#ffffff" stroke-width="3"/>
  
  <!-- Chart dots -->
  <circle cx="70" cy="200" r="3" fill="#ffffff"/>
  <circle cx="90" cy="180" r="3" fill="#ffffff"/>
  <circle cx="110" cy="190" r="3" fill="#ffffff"/>
  <circle cx="130" cy="160" r="3" fill="#ffffff"/>
  <circle cx="150" cy="170" r="3" fill="#ffffff"/>
  <circle cx="170" cy="140" r="3" fill="#ffffff"/>
  <circle cx="190" cy="150" r="3" fill="#ffffff"/>
  <circle cx="210" cy="120" r="3" fill="#ffffff"/>
  <circle cx="230" cy="130" r="3" fill="#ffffff"/>
  <circle cx="250" cy="100" r="3" fill="#ffffff"/>
  <circle cx="270" cy="110" r="3" fill="#ffffff"/>
  <circle cx="290" cy="90" r="3" fill="#ffffff"/>
  <circle cx="310" cy="100" r="3" fill="#ffffff"/>
  <circle cx="330" cy="80" r="3" fill="#ffffff"/>
  
  <!-- Prediction line -->
  <polyline points="330,80 350,70 370,60" 
            fill="none" stroke="#fbbf24" stroke-width="2" stroke-dasharray="5,5"/>
  
  <!-- Price indicator -->
  <rect x="280" y="60" width="60" height="25" fill="#1f2937" rx="4"/>
  <text x="310" y="75" text-anchor="middle" fill="#fbbf24" font-family="Arial" font-size="12" font-weight="bold">$150</text>
  
  <text x="200" y="250" text-anchor="middle" fill="#374151" font-family="Arial" font-size="16" font-weight="bold">Stock Prediction ML</text>
</svg>
